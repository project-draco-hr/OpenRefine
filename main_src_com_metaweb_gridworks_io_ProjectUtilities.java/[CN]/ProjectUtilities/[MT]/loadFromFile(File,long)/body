{
  ZipFile zipFile=new ZipFile(file);
  try {
    Pool pool=new Pool();
    ZipEntry poolEntry=zipFile.getEntry("pool.txt");
    if (poolEntry != null) {
      pool.load(new InputStreamReader(zipFile.getInputStream(poolEntry)));
    }
    return Project.loadFromReader(new LineNumberReader(new InputStreamReader(zipFile.getInputStream(zipFile.getEntry("data.txt")))),id,pool);
  }
  finally {
    zipFile.close();
  }
}
