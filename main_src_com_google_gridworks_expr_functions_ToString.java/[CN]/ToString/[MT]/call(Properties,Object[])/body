{
  if (args.length >= 1) {
    Object o1=args[0];
    if (o1 != null) {
      if (o1 instanceof Calendar) {
        if (args.length == 2) {
          Object o2=args[1];
          if (o2 != null && o2 instanceof String) {
            SimpleDateFormat formatter=new SimpleDateFormat((String)o2);
            return formatter.format(((Calendar)o1).getTime());
          }
        }
      }
 else {
        return (o1 instanceof String) ? o1 : o1.toString();
      }
    }
  }
  return null;
}
