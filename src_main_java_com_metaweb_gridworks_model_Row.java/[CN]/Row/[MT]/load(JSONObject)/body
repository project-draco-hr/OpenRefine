{
  JSONArray a=obj.getJSONArray("cells");
  int count=a.length();
  Row row=new Row(count);
  for (int i=0; i < count; i++) {
    if (!a.isNull(i)) {
      JSONObject o=a.getJSONObject(i);
      row.setCell(i,Cell.load(o));
    }
  }
  if (obj.has(STARRED)) {
    row.starred=obj.getBoolean(STARRED);
  }
  if (obj.has(FLAGGED)) {
    row.flagged=obj.getBoolean(FLAGGED);
  }
  return row;
}
