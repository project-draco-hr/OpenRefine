{
  writer.object();
  writer.key("entries");
  writer.object();
  for (  String k : _prefs.keySet()) {
    writer.key(k);
    Object o=_prefs.get(k);
    if (o instanceof Jsonizable) {
      ((Jsonizable)o).write(writer,options);
    }
 else {
      writer.value(o);
    }
  }
  writer.endObject();
  writer.endObject();
}
