{
  writer.object();
  if (ExpressionUtils.isError(value)) {
    writer.key("e");
    writer.value(((EvalError)value).message);
  }
 else {
    writer.key("v");
    if (value != null && value instanceof Calendar) {
      writer.value(ParsingUtilities.dateToString(((Calendar)value).getTime()));
    }
 else {
      writer.value(value);
    }
  }
  if (recon != null) {
    writer.key("r");
    recon.write(writer,options);
  }
  writer.endObject();
}
