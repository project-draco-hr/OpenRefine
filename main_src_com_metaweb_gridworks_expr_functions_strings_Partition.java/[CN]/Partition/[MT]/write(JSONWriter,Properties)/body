{
  writer.object();
  writer.key("description");
  writer.value("Returns an array of strings [a,frag,b] where a is the string part before the first occurrence of frag in s and b is what's left. If omitFragment is true, frag is not returned.");
  writer.key("params");
  writer.value("string s, string or regex frag, optional boolean omitFragment");
  writer.key("returns");
  writer.value("array");
  writer.endObject();
}
