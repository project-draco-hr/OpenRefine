{
  writer.object();
  writer.key("op");
  writer.value("recon");
  writer.key("description");
  writer.value("Reconcile cells in column " + _columnName + " to type "+ _typeID);
  writer.key("columnName");
  writer.value(_columnName);
  writer.key("typeID");
  writer.value(_typeID);
  writer.endObject();
}
