{
  writer.write(Gridworks.s_version);
  writer.write('\n');
  writer.write("columnModel=\n");
  columnModel.save(writer,options);
  writer.write("history=\n");
  history.save(writer,options);
  if (protograph != null) {
    writer.write("protograph=");
    protograph.save(writer,options);
    writer.write('\n');
  }
  writer.write("rowCount=");
  writer.write(Integer.toString(rows.size()));
  writer.write('\n');
  for (  Row row : rows) {
    row.save(writer);
    writer.write('\n');
  }
}
