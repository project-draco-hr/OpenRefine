{
  JSONArray a=o.getJSONArray("facets");
  int length=a.length();
  for (int i=0; i < length; i++) {
    JSONObject fo=a.getJSONObject(i);
    String type=fo.has("type") ? fo.getString("type") : "list";
    Facet facet=null;
    if ("list".equals(type)) {
      facet=new ListFacet();
    }
    if (facet != null) {
      facet.initializeFromJSON(fo);
      _facets.add(facet);
    }
  }
}
