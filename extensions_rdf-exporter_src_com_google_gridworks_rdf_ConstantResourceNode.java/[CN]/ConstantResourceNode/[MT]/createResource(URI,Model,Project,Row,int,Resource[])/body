{
  if (this.uri != null & this.uri.length() > 0) {
    String tmp;
    try {
      tmp=Util.getUri(baseUri,this.uri);
    }
 catch (    URISyntaxException e) {
      e.printStackTrace();
      return null;
    }
    Resource r=model.createResource(tmp);
    return r;
  }
 else {
    return null;
  }
}
