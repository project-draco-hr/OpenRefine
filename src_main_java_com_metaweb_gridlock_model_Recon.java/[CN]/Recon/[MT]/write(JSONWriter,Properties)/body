{
  writer.object();
  writer.key("j");
  writer.value(judgmentToString());
  if (match != null) {
    writer.key("m");
    match.write(writer,options);
  }
 else {
    writer.key("c");
    writer.array();
    for (    ReconCandidate c : candidates) {
      c.write(writer,options);
    }
    writer.endArray();
  }
  writer.endObject();
}
