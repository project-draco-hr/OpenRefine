{
  if (obj == null) {
    obj=new JSONObject();
    try {
      obj.put("value",cell.value.toString());
      if (node.lang != null) {
        obj.put("lang",node.lang);
      }
      obj.put("connect","insert");
    }
 catch (    JSONException e) {
      e.printStackTrace();
    }
  }
  return obj;
}
