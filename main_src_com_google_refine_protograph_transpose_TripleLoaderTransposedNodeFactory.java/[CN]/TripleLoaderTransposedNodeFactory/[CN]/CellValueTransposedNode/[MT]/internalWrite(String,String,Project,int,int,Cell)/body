{
  if (subject != null) {
    if ("/type/text".equals(node.lang)) {
      writeLine(subject,predicate,cell.value,node.lang,project,subjectRowIndex,subjectCellIndex,subjectCell,!load);
    }
 else {
      writeLine(subject,predicate,cell.value,project,subjectRowIndex,subjectCellIndex,subjectCell,-1,-1,null,!load);
    }
  }
  return cell.value;
}
