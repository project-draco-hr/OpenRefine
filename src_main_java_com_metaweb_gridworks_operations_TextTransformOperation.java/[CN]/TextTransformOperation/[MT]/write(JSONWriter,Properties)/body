{
  writer.object();
  writer.key("op");
  writer.value(OperationRegistry.s_opClassToName.get(this.getClass()));
  writer.key("description");
  writer.value(getBriefDescription(null));
  writer.key("engineConfig");
  writer.value(getEngineConfig());
  writer.key("columnName");
  writer.value(_columnName);
  writer.key("expression");
  writer.value(_expression);
  writer.key("onError");
  writer.value(onErrorToString(_onError));
  writer.key("repeat");
  writer.value(_repeat);
  writer.key("repeatCount");
  writer.value(_repeatCount);
  writer.endObject();
}
