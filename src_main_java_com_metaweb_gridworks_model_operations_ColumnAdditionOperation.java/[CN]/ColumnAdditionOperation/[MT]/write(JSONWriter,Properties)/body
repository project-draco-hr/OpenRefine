{
  writer.object();
  writer.key("op");
  writer.value("add-column");
  writer.key("description");
  writer.value("Create column " + _headerLabel + " at index "+ _columnInsertIndex+ " based on column "+ _baseColumnName+ " using expression "+ _expression);
  writer.key("engineConfig");
  writer.value(_engineConfig);
  writer.key("headerLabel");
  writer.value(_headerLabel);
  writer.key("columnInsertIndex");
  writer.value(_columnInsertIndex);
  writer.key("baseColumnName");
  writer.value(_baseColumnName);
  writer.key("expression");
  writer.value(_expression);
  writer.endObject();
}
