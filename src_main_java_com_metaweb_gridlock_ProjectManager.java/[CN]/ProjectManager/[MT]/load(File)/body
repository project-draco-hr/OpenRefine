{
  ProjectManager pm=null;
  FileInputStream fis=null;
  ObjectInputStream in=null;
  try {
    fis=new FileInputStream(file);
    in=new ObjectInputStream(fis);
    pm=(ProjectManager)in.readObject();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
catch (  ClassNotFoundException e) {
    e.printStackTrace();
  }
 finally {
    if (fis != null) {
      try {
        fis.close();
      }
 catch (      Exception e) {
      }
    }
    if (in != null) {
      try {
        in.close();
      }
 catch (      Exception e) {
      }
    }
  }
  return pm;
}
