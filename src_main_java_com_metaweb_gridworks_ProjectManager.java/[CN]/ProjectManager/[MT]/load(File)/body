{
  try {
    return loadFromFile(new File(dir,"projects.json"));
  }
 catch (  Exception e) {
  }
  try {
    return loadFromFile(new File(dir,"projects.json.temp"));
  }
 catch (  Exception e) {
  }
  try {
    return loadFromFile(new File(dir,"projects.json.old"));
  }
 catch (  Exception e) {
  }
  return null;
}
