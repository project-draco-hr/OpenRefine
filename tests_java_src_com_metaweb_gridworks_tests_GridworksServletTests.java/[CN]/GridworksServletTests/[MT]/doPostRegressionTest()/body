{
  whenGetCommandNameThenReturn(TEST_COMMAND_PATH);
  try {
    SUT.wrapDoPost(request,response);
  }
 catch (  ServletException e) {
    Assert.fail();
  }
catch (  IOException e) {
    Assert.fail();
  }
  verifyGetCommandNameCalled();
  try {
    verify(command,times(1)).doPost(request,response);
  }
 catch (  ServletException e) {
    Assert.fail();
  }
catch (  IOException e) {
    Assert.fail();
  }
}
