{
  writer.object();
  writer.key("nodeType");
  writer.value("cell-as-topic");
  writer.key("columnName");
  writer.value(columnName);
  writer.key("createForNoReconMatch");
  writer.value(createForNoReconMatch);
  if (createForNoReconMatch && type != null) {
    writer.key("type");
    type.write(writer,options);
  }
  if (links != null) {
    writer.key("links");
    writer.array();
    for (    Link link : links) {
      link.write(writer,options);
    }
    writer.endArray();
  }
  writer.endObject();
}
