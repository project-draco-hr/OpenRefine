{
  int[] cftab=new int[257];
  char ch;
  cftab[0]=0;
  for (i=1; i <= 256; i++) {
    cftab[i]=unzftab[i - 1];
  }
  for (i=1; i <= 256; i++) {
    cftab[i]+=cftab[i - 1];
  }
  for (i=0; i <= last; i++) {
    ch=(char)ll8[i];
    tt[cftab[ch]]=i;
    cftab[ch]++;
  }
  cftab=null;
  tPos=tt[origPtr];
  count=0;
  i2=0;
  ch2=256;
  if (blockRandomised) {
    rNToGo=0;
    rTPos=0;
    setupRandPartA();
  }
 else {
    setupNoRandPartA();
  }
}
