{
  if ("min".equals(_mode)) {
    return new ExpressionNumberComparisonRowFilter(_eval,_cellIndex){
      protected boolean checkValue(      double d){
        return d >= _min;
      }
    }
;
  }
 else   if ("max".equals(_mode)) {
    return new ExpressionNumberComparisonRowFilter(_eval,_cellIndex){
      protected boolean checkValue(      double d){
        return d <= _max;
      }
    }
;
  }
 else {
    return new ExpressionNumberComparisonRowFilter(_eval,_cellIndex){
      protected boolean checkValue(      double d){
        return d >= _min && d <= _max;
      }
    }
;
  }
}
