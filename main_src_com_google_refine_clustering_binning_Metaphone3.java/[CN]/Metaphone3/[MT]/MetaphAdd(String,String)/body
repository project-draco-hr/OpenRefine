{
  if (!(main.equals("A") && (m_primary.length() > 0) && (m_primary.charAt(m_primary.length() - 1) == 'A'))) {
    m_primary.append(main);
  }
  if (!(alt.equals("A") && (m_secondary.length() > 0) && (m_secondary.charAt(m_secondary.length() - 1) == 'A'))) {
    if (!alt.isEmpty()) {
      m_secondary.append(alt);
    }
  }
}
