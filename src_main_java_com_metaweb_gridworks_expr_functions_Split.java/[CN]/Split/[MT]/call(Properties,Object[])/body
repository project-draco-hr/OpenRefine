{
  if (args.length == 2) {
    Object v=args[0];
    Object split=args[1];
    if (v != null && split != null && split instanceof String) {
      return (v instanceof String ? (String)v : v.toString()).split((String)split);
    }
  }
  return new EvalError(ControlFunctionRegistry.getFunctionName(this) + " expects 2 strings");
}
