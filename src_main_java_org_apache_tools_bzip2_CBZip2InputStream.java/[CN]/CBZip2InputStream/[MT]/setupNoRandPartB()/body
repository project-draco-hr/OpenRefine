{
  if (ch2 != chPrev) {
    currentState=NO_RAND_PART_A_STATE;
    count=1;
    setupNoRandPartA();
  }
 else {
    count++;
    if (count >= 4) {
      z=ll8[tPos];
      tPos=tt[tPos];
      currentState=NO_RAND_PART_C_STATE;
      j2=0;
      setupNoRandPartC();
    }
 else {
      currentState=NO_RAND_PART_A_STATE;
      setupNoRandPartA();
    }
  }
}
