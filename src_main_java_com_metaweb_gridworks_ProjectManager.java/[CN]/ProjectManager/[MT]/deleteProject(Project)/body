{
  if (_projectsMetadata.containsKey(project.id)) {
    _projects.remove(project.id);
    _projectsMetadata.remove(project.id);
    File file=new File(getProjectDir(project.id),"raw-data");
    if (file.exists()) {
      file.delete();
    }
    save();
    return true;
  }
 else {
    return false;
  }
}
