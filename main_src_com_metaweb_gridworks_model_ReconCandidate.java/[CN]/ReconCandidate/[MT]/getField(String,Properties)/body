{
  if ("id".equals(name)) {
    return topicID;
  }
 else   if ("guid".equals(name)) {
    return topicGUID;
  }
 else   if ("name".equals(name)) {
    return topicName;
  }
 else   if ("type".equals(name)) {
    return typeIDs;
  }
 else   if ("score".equals(name)) {
    return score;
  }
  return null;
}
