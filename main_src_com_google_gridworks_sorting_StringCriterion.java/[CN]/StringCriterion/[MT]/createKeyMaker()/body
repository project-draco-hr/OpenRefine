{
  return new KeyMaker(){
    @Override protected Object makeKey(    Object value){
      return (ExpressionUtils.isNonBlankData(value) && !(value instanceof String)) ? value.toString() : value;
    }
    @Override public int compareKeys(    Object key1,    Object key2){
      if (StringCriterion.this.caseSensitive) {
        return ((String)key1).compareTo((String)key2);
      }
 else {
        return ((String)key1).compareToIgnoreCase((String)key2);
      }
    }
  }
;
}
