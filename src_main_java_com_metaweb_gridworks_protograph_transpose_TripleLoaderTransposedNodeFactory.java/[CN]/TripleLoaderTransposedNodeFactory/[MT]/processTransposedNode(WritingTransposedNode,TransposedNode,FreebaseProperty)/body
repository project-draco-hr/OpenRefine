{
  if (parentNode != null) {
    if (parentNode instanceof TransposedNodeWithChildren) {
      TransposedNodeWithChildren parentNode2=(TransposedNodeWithChildren)parentNode;
      parentNode2.children.add(tnode);
      parentNode2.properties.add(property);
    }
  }
