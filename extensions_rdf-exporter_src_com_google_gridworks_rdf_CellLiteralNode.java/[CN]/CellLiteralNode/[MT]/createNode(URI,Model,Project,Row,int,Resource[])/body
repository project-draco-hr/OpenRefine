{
  String val;
  try {
    val=row.getCell(this.columnIndex).value.toString();
  }
 catch (  NullPointerException ne) {
    return null;
  }
  if (val != null && val.length() > 0) {
    Literal l;
    if (this.valueType != null) {
      l=model.createTypedLiteral(val,valueType);
    }
 else {
      l=model.createLiteral(val);
    }
    return l;
  }
 else {
    return null;
  }
}
