{
  writer.object();
  writer.key("op");
  writer.value("disapprove-recon");
  writer.key("description");
  writer.value("Discard recon judgments in column " + _columnName);
  writer.key("engineConfig");
  writer.value(getEngineConfig());
  writer.key("columnName");
  writer.value(_columnName);
  writer.endObject();
}
