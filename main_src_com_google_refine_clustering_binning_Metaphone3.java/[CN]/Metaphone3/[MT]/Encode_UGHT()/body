{
  if (StringAt((m_current - 1),4,"UGHT","")) {
    if ((StringAt((m_current - 3),5,"LAUGH","") && !(StringAt((m_current - 4),7,"SLAUGHT","") || StringAt((m_current - 3),7,"LAUGHTO",""))) || StringAt((m_current - 4),6,"DRAUGH","")) {
      MetaphAdd("FT");
    }
 else {
      MetaphAdd("T");
    }
    m_current+=3;
    return true;
  }
  return false;
}
