{
  int i;
  int rNToGo=0;
  int rTPos=0;
  for (i=0; i < 256; i++) {
    inUse[i]=false;
  }
  for (i=0; i <= last; i++) {
    if (rNToGo == 0) {
      rNToGo=(char)rNums[rTPos];
      rTPos++;
      if (rTPos == 512) {
        rTPos=0;
      }
    }
    rNToGo--;
    block[i + 1]^=((rNToGo == 1) ? 1 : 0);
    block[i + 1]&=0xFF;
    inUse[block[i + 1]]=true;
  }
}
