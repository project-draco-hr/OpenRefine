{
  if (args.length == 1 && args[0] != null) {
    if (args[0] instanceof Number) {
      return args[0];
    }
 else {
      String s=args[0].toString();
      try {
        return Double.parseDouble(s);
      }
 catch (      NumberFormatException e) {
        return new EvalError("Cannot parse to number");
      }
    }
  }
  return null;
}
