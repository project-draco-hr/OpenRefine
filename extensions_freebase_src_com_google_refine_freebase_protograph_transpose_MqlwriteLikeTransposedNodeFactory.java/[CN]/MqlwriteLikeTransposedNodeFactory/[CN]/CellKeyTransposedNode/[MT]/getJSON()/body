{
  if (obj == null) {
    obj=new JSONObject();
    try {
      obj.put(VALUE,cell.value.toString());
      JSONObject nsObj=new JSONObject();
      nsObj.put(ID,node.namespace.id);
      obj.put("namespace",nsObj);
      obj.put(CONNECT,"insert");
    }
 catch (    JSONException e) {
      e.printStackTrace();
    }
  }
  return obj;
}
