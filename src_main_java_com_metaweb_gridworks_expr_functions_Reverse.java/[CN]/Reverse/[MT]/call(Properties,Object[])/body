{
  if (args.length == 1) {
    Object v=args[0];
    if (v != null && v.getClass().isArray()) {
      Object[] a=(Object[])v;
      Object[] r=new Object[a.length];
      for (int i=0; i < a.length; i++) {
        r[i]=a[r.length - i - 1];
      }
      return r;
    }
  }
  return new EvalError(ControlFunctionRegistry.getFunctionName(this) + " expects an array");
}
