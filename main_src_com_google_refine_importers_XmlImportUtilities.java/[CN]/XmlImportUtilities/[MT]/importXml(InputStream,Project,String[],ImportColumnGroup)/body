{
  try {
    TreeParser parser=new XmlParser(inputStream);
    while (parser.hasNext()) {
      int eventType=parser.next();
      if (eventType == XMLStreamConstants.START_ELEMENT) {
        findRecord(project,parser,recordPath,0,rootColumnGroup);
      }
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
