{
  str=normalize(str);
  List<Token> tokens=new ArrayList<Token>();
  for (int i=0; i < str.length(); i++) {
    int index=i + ngram_size;
    if (index <= str.length()) {
      tokens.add(intern(str.substring(i,index)));
    }
  }
  return (Token[])tokens.toArray(new BasicToken[tokens.size()]);
}
