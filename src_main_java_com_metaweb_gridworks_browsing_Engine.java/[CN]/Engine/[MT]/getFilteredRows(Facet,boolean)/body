{
  ConjunctiveFilteredRows cfr=new ConjunctiveFilteredRows(includeContextual,_includeDependent);
  for (  Facet facet : _facets) {
    if (facet != except) {
      RowFilter rowFilter=facet.getRowFilter();
      if (rowFilter != null) {
        cfr.add(rowFilter);
      }
    }
  }
  return cfr;
}
