{
  StringBuilder sb=new StringBuilder();
  for (  Column c : project.columnModel.columns) {
    sb.append(c.getName());
    sb.append("; ");
  }
  logger.info(sb.toString());
  for (  Row r : project.rows) {
    sb=new StringBuilder();
    for (    Cell c : r.cells) {
      if (c != null) {
        sb.append(c.value);
        sb.append("; ");
      }
 else {
        sb.append("null; ");
      }
    }
    logger.info(sb.toString());
  }
}
