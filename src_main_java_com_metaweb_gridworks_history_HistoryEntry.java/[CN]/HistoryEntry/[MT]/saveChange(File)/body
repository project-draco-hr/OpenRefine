{
  ZipOutputStream out=new ZipOutputStream(new FileOutputStream(file));
  try {
    out.putNextEntry(new ZipEntry("change.txt"));
    try {
      Writer writer=new OutputStreamWriter(out);
      try {
        History.writeOneChange(writer,_change);
      }
  finally {
        writer.flush();
      }
    }
  finally {
      out.closeEntry();
    }
  }
  finally {
    out.close();
  }
}
