{
  URL feedUrl=new URL("https://docs.google.com/feeds/default/private/full");
  Query query=new Query(feedUrl);
  query.addCategoryFilter(new Query.CategoryFilter(new Category("http://schemas.google.com/g/2005#kind",type)));
  DocumentListFeed feed=service.query(query,DocumentListFeed.class);
  for (  DocumentListEntry entry : feed.getEntries()) {
    writer.object();
    writer.key("docId");
    writer.value(entry.getId());
    writer.key("docLink");
    writer.value(entry.getHtmlLink().getHref());
    writer.key("docSelfLink");
    writer.value(entry.getSelfLink().getHref());
    writer.key("title");
    writer.value(entry.getTitle().getPlainText());
    writer.key("type");
    writer.value(entry.getType());
    DateTime updated=entry.getUpdated();
    if (updated != null) {
      writer.key("updated");
      writer.value(updated.toStringRfc822());
    }
    writer.key("authors");
    writer.array();
    for (    Person person : entry.getAuthors()) {
      writer.value(person.getName());
    }
    writer.endArray();
    writer.endObject();
  }
}
