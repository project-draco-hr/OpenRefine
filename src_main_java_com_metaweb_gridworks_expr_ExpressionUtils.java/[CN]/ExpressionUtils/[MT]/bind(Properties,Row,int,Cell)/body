{
  bindings.put("row",row);
  bindings.put("rowIndex",rowIndex);
  bindings.put("cells",row.getField("cells",bindings));
  if (cell == null) {
    bindings.remove("cell");
    bindings.remove("value");
  }
 else {
    bindings.put("cell",cell);
    if (cell.value == null) {
      bindings.remove("value");
    }
 else {
      bindings.put("value",cell.value);
    }
  }
}
