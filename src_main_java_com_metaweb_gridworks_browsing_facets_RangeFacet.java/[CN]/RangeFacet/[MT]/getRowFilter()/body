{
  if (_eval != null && _errorMessage == null && _selected) {
    if ("min".equals(_mode)) {
      return new ExpressionNumberComparisonRowFilter(_eval,_cellIndex,_selectNumeric,_selectNonNumeric,_selectBlank,_selectError){
        protected boolean checkValue(        double d){
          return d >= _from;
        }
      }
;
    }
 else     if ("max".equals(_mode)) {
      return new ExpressionNumberComparisonRowFilter(_eval,_cellIndex,_selectNumeric,_selectNonNumeric,_selectBlank,_selectError){
        protected boolean checkValue(        double d){
          return d <= _to;
        }
      }
;
    }
 else {
      return new ExpressionNumberComparisonRowFilter(_eval,_cellIndex,_selectNumeric,_selectNonNumeric,_selectBlank,_selectError){
        protected boolean checkValue(        double d){
          return d >= _from && d <= _to;
        }
      }
;
    }
  }
 else {
    return null;
  }
}
