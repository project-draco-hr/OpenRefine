{
  super.destroy();
  if (projectStore != null) {
    projectStore.close();
    projectById=null;
  }
  if (lockStore != null) {
    lockStore.close();
    lockByProject=null;
  }
  if (timer != null) {
    timer.cancel();
    timer.purge();
    timer=null;
  }
  if (env != null) {
    env.close();
    env=null;
  }
}
