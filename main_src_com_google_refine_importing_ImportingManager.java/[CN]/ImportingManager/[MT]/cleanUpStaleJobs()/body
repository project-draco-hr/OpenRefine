{
  long now=System.currentTimeMillis();
  for (  Long id : new HashSet<Long>(jobs.keySet())) {
    ImportingJob job=jobs.get(id);
    if (job != null && !job.updating && now - job.lastTouched > s_stalePeriod) {
      job.dispose();
      jobs.remove(id);
      logger.info("Disposed " + id);
    }
  }
}
