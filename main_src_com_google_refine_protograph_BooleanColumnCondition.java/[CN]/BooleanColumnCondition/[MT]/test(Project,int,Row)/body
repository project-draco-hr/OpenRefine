{
  Column column=project.columnModel.getColumnByName(columnName);
  if (column != null) {
    Object o=row.getCellValue(column.getCellIndex());
    if (o != null) {
      if (o instanceof Boolean) {
        return ((Boolean)o).booleanValue();
      }
 else {
        return Boolean.parseBoolean(o.toString());
      }
    }
  }
  return false;
}
