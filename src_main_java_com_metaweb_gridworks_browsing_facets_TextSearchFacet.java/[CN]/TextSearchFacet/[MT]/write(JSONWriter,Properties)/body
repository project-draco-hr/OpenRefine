{
  writer.object();
  writer.key("name");
  writer.value(_name);
  writer.key("cellIndex");
  writer.value(_cellIndex);
  writer.key("query");
  writer.value(_query);
  writer.key("mode");
  writer.value(_mode);
  writer.key("caseSensitive");
  writer.value(_caseSensitive);
  writer.endObject();
}
