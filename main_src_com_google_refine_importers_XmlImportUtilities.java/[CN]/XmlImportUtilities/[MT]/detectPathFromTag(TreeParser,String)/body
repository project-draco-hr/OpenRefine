{
  try {
    while (parser.hasNext()) {
      TreeParserToken eventType=parser.next();
      if (eventType == TreeParserToken.StartEntity) {
        List<String> path=detectRecordElement(parser,tag);
        if (path != null) {
          String[] path2=new String[path.size()];
          path.toArray(path2);
          return path2;
        }
      }
    }
  }
 catch (  Exception e) {
  }
  return null;
}
