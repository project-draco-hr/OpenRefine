{
  _name=o.getString("name");
  _expression=o.getString("expression");
  _cellIndex=o.getInt("cellIndex");
  _eval=new Parser(_expression).getExpression();
  _mode=o.getString("mode");
  if ("min".equals(_mode)) {
    _from=o.getDouble("from");
  }
 else   if ("max".equals(_mode)) {
    _to=o.getDouble("to");
  }
 else {
    _from=o.getDouble("from");
    _to=o.getDouble("to");
  }
}
