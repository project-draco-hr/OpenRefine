{
  ZipOutputStream out=new ZipOutputStream(new FileOutputStream(file));
  try {
    Pool pool=new Pool();
    out.putNextEntry(new ZipEntry("change.txt"));
    try {
      History.writeOneChange(out,_change,pool);
    }
  finally {
      out.closeEntry();
    }
    out.putNextEntry(new ZipEntry("pool.txt"));
    try {
      pool.save(out);
    }
  finally {
      out.closeEntry();
    }
  }
  finally {
    out.close();
  }
}
