{
synchronized (this) {
    if (_projectsMetadata.containsKey(projectID)) {
      _projectsMetadata.remove(projectID);
    }
    if (_projects.containsKey(projectID)) {
      _projects.remove(projectID);
    }
    File dir=getProjectDir(projectID);
    if (dir.exists()) {
      dir.delete();
    }
  }
  saveWorkspace();
}
