{
synchronized (this) {
    File projectDir=getProjectDir(id);
    ProjectMetadata metadata=_projectsMetadata.get(id);
    if (metadata != null) {
      try {
        metadata.save(projectDir);
      }
 catch (      Exception e) {
        e.printStackTrace();
      }
    }
    Project project=_projects.get(id);
    if (project != null && metadata.getModified().after(project.lastSave)) {
      try {
        project.save();
      }
 catch (      Exception e) {
        e.printStackTrace();
      }
    }
  }
}
