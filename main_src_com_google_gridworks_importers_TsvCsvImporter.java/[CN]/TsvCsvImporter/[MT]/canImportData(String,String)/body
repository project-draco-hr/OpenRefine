{
  if (contentType != null) {
    contentType=contentType.toLowerCase().trim();
    return "text/plain".equals(contentType) || "text/csv".equals(contentType) || "text/x-csv".equals(contentType)|| "text/tab-separated-value".equals(contentType);
  }
 else   if (fileName != null) {
    fileName=fileName.toLowerCase();
    if (fileName.endsWith(".tsv")) {
      return true;
    }
 else     if (fileName.endsWith(".csv")) {
      return true;
    }
  }
  return false;
}
