{
  writer.object();
  writeNode(writer,options);
  writer.key("rdfTypes");
  writer.array();
  for (  RdfType type : this.getTypes()) {
    writer.object();
    writer.key("uri");
    writer.value(type.uri);
    writer.key("curie");
    writer.value(type.curie);
    writer.endObject();
  }
  writer.endArray();
  writer.key("links");
  writer.array();
  for (int i=0; i < getLinkCount(); i++) {
    Link l=getLink(i);
    l.write(writer,options);
  }
  writer.endArray();
  writer.endObject();
}
