{
  writer.object();
  writer.key("id");
  writer.value(id);
  writer.key("name");
  writer.value(name);
  writer.key("score");
  writer.value(score);
{
    writer.key("types");
    writer.array();
    for (    String typeID : types) {
      writer.value(typeID);
    }
    writer.endArray();
  }
  writer.endObject();
}
