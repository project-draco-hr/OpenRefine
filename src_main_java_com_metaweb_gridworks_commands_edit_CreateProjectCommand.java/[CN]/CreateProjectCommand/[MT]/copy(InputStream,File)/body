{
  FileOutputStream output=new FileOutputStream(file);
  byte[] buffer=new byte[4 * 1024];
  long count=0;
  int n=0;
  try {
    while (-1 != (n=input.read(buffer))) {
      output.write(buffer,0,n);
      count+=n;
    }
  }
  finally {
    try {
      output.close();
    }
 catch (    IOException e) {
    }
    try {
      input.close();
    }
 catch (    IOException e) {
    }
  }
  return count;
}
