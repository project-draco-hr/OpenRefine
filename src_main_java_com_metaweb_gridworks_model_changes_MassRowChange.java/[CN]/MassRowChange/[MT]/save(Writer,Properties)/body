{
  writer.write("newRowCount=");
  writer.write(Integer.toString(_newRows.size()));
  writer.write('\n');
  for (  Row row : _newRows) {
    row.save(writer);
    writer.write('\n');
  }
  writer.write("oldRowCount=");
  writer.write(Integer.toString(_oldRows.size()));
  writer.write('\n');
  for (  Row row : _oldRows) {
    row.save(writer);
    writer.write('\n');
  }
  writer.write("/ec/\n");
}
